{
  "name": "RaveboxUI",
  "version": "0.0.1",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.9.9",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.48",
    "@material-ui/styles": "4.9.6",
    "clsx": "1.1.0",
    "connected-react-router": "6.7.0",
    "env-cmd": "10.1.0",
    "express": "4.17.1",
    "history": "4.10.1",
    "isomorphic-fetch": "2.2.1",
    "jest": "24.9.0",
    "lodash.debounce": "4.0.8",
    "moment": "2.24.0",
    "notistack": "0.9.9",
    "razzle": "3.0.0",
    "razzle-plugin-typescript": "3.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-dropzone": "10.2.2",
    "react-frontload": "1.1.0",
    "react-helmet": "5.2.1",
    "react-player": "1.15.3",
    "react-redux": "7.2.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-transition-group": "4.3.0",
    "react-truncate": "2.4.0",
    "redux": "4.0.5",
    "redux-observable": "1.2.0",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",
    "serialize-javascript": "3.0.0",
    "typesafe-actions": "5.1.0",
    "universal-cookie": "4.0.3",
    "use-resize-observer": "6.1.0-alpha.3"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "start:local": "razzle start --inspect=0.0.0.0:9229",
    "build:development": "env-cmd .env.development razzle build",
    "start:staging": "npm run build:staging && npm run serve:staging",
    "start-debug:staging": "env-cmd -f .env.staging razzle start --inspect=0.0.0.0:9229",
    "build:staging": "env-cmd -f .env.staging razzle build",
    "serve:staging": "env-cmd -f .env.staging node build/server.js",
    "test": "razzle test --env=jsdom",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/enzyme-to-json": "1.5.3",
    "@types/express": "4.17.2",
    "@types/history": "4.7.5",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "24.9.0",
    "@types/lodash.debounce": "4.0.6",
    "@types/node": "13.9.8",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-frontload": "1.0.0",
    "@types/react-helmet": "5.0.15",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/react-transition-group": "4.2.4",
    "@types/react-truncate": "2.3.3",
    "@types/serialize-javascript": "1.5.0",
    "@types/universal-cookie": "2.2.0",
    "@types/webpack-env": "1.15.1",
    "@svgr/webpack": "5.3.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "node-sass-chokidar": "1.4.0",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "typescript": "3.8.2"
  },
  "resolutions": {
    "@types/node": "13.9.8"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}

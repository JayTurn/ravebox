{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "react",
      "host": {
        "sourcePath": "/var/app/current/client"
      }
    },
    {
      "name": "react_modules",
      "host": {
        "sourcePath": "/var/app/current/client/node_modules"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "react_client",
      "image": "jayturn/react:1.1.0",
      "command": ["yarn", "start:staging"],
      "essential": true,
      "memory": 900,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3000
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "react",
          "containerPath": "/usr/src/client/app",
          "readOnly": true
        },
        {
          "sourceVolume": "react_modules",
          "containerPath": "/usr/src/client/node_modules"
        }
      ]
    }
  ]
}

{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "server",
      "host": {
        "sourcePath": "/var/app/current/server/."
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "node_server",
      "image": "jayturn/node-server:1.3.0",
      "command": ["yarn", "start:staging"],
      "essential": true,
      "memoryReservation": 256,
      "portMappings": [
        {
          "hostPort": 9000,
          "containerPort": 9000
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "server",
          "containerPath": "/server/app"
        }
      ]
    }
  ]
}

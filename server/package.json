{
  "name": "RaveboxAPI",
  "private": true,
  "version": "0.0.1",
  "description": "Secret",
  "author": "Jay Turnbull",
  "scripts": {
    "start": "npm run build && npm run serve-debug",
    "start-debug": "npm run build && npm run serve-debug",
    "build": "npm run build-ts && npm run eslint",
    "serve": "nodemon --verbose dist/server.js",
    "serve-debug": "nodemon -L --inspect=0.0.0.0:9229 --verbose dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve\"",
    "test": "jest --forceExit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "debug": "npm run build && npm run watch-debug",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "async": "3.2.0",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "compose-middleware": "5.0.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "ejs": "3.0.1",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "express-flash": "0.0.2",
    "express-jwt": "5.3.1",
    "express-validator": "6.4.0",
    "glob": "7.1.6",
    "json2csv": "4.5.4",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "lusca": "1.6.1",
    "mongoose": "5.9.2",
    "morgan": "1.9.1",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "underscore.string": "3.3.5",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.2"
  },
  "devDependencies": {
    "@types/async": "3.0.8",
    "@types/body-parser": "1.19.0",
    "@types/cookie-parser": "1.4.2",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.6",
    "@types/ejs": "3.0.1",
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.2",
    "@types/express-jwt": "0.0.42",
    "@types/glob": "7.1.1",
    "@types/lodash": "4.14.149",
    "@types/json2csv": "4.5.1",
    "@types/jsonwebtoken": "8.3.7",
    "@types/mongodb": "3.3.16",
    "@types/mongoose": "5.7.2",
    "@types/morgan": "1.7.37",
    "@types/node": "12.12.6",
    "@types/passport": "1.0.3",
    "@types/passport-local": "1.0.33",
    "@types/rx": "4.1.1",
    "@types/underscore.string" : "0.0.38",
    "@typescript-eslint/parser": "2.21.0",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "concurrently": "5.1.0",
    "eslint": "6.8.0",
    "nodemon": "2.0.2",
    "typescript": "3.8.2"
  }
}
